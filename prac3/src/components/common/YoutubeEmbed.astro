---
import { YouTube } from 'astro-embed';

interface Props {
  url: string;
  title: string;
  playLabel: string;
  class: string;
}

const { url, playLabel, title, class: className } = Astro.props;
---

<YouTube
  id={url}
  title={title}
  playlabel={playLabel}
  posterQuality="max"
  class={className}
/>
