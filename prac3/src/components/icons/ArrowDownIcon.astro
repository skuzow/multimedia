---
interface Props {
  size?: string;
}

const { size = 20 } = Astro.props;
---

<svg
  class="rotate-90 stroke-current"
  height={size}
  width={size}
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  stroke-width="1.5"
  fill="none"
  stroke-linecap="round"
  stroke-linejoin="round"
>
  <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
  <path d="M9 6l6 6l-6 6"></path>
</svg>
